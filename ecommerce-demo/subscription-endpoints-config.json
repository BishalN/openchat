    {
        "name": "Get Subscription Status",
        "whenToUse": "When you need to check the current subscription status and plan details for a user",
        "config": {
            "body": [],
            "apiUrl": "http://localhost:3001/api/subscriptions/status",
            "headers": [
                {
                    "key": "x-user-id",
                    "value": "{{userId}}"
                }
            ],
            "apiMethod": "GET",
            "dataInputs": [
                {
                    "name": "userId",
                    "type": "Text",
                    "array": false,
                    "description": "The user ID to check subscription status for"
                }
            ],
            "parameters": [],
            "dataAccessType": "full"
        }
    },
    {
        "name": "Upgrade Subscription Plan",
        "whenToUse": "When you need to upgrade a user's subscription plan to a higher tier",
        "config": {
            "body": [],
            "apiUrl": "http://localhost:3001/api/subscriptions/upgrade",
            "headers": [
                {
                    "key": "x-user-id",
                    "value": "{{userId}}"
                },
                {
                    "key": "x-plan",
                    "value": "{{plan}}"
                }
            ],
            "apiMethod": "POST",
            "dataInputs": [
                {
                    "name": "userId",
                    "type": "Text",
                    "array": false,
                    "description": "The user ID to upgrade the subscription for"
                },
                {
                    "name": "plan",
                    "type": "Text",
                    "array": false,
                    "description": "The new plan to upgrade to (free, premium, or enterprise)"
                }
            ],
            "parameters": [],
            "dataAccessType": "full"
        }
    },
